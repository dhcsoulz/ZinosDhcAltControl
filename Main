local altPositions = {
    [2377457818] = Vector3.new(-265, 21.47, -402),  -- Replace 1234567890 with the alt's ID and Vector3.new() with alt's position
    [2339222369] = Vector3.new(-275, 21.47, -402),  -- Replace 2345678901 with the next alt's ID and Vector3.new() with alt's position
